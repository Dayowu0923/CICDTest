pipeline {
    agent any
    tools {
        dotnetsdk  '1111'
    }
    stages {
        stage('Build') {
   
            steps {

                sh "dotnet sonarscanner begin /k:projectkey1"
sh "dotnet build"
sh "dotnet sonarscanner end"

                
            }
        }
        stage('Test') {
            steps {
                echo 'Running Tests...'
                sh 'dotnet test' // °õ¦æ dotnet test
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying...'
              
            }
        }
    }

} 